{
  "name": "already-styled-components",
  "version": "1.0.0",
  "description": "A collection of common UI components styled with styled-components.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "prepublish": "npm run lint && npm run build",
    "lint": "./node_modules/.bin/eslint src",
    "lint-fix": "./node_modules/.bin/eslint --fix src",
    "test": "jest"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "react",
    "styled-components",
    "onecolor",
    "fortawesome"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkosKon/already-styled-components.git"
  },
  "author": "AffectionateDoor",
  "license": "MIT",
  "dependencies": {
    "onecolor": "^3.0.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "styled-components": "^3.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^22.4.3",
    "pre-commit": "^1.2.2",
    "react-dom": "^16.4.2"
  }
}
