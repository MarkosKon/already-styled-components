{
  "name": "already-styled-components",
  "version": "3.1.2",
  "description": "React UI components styled with styled-components.",
  "module": "dist/already-styled-components.esm.js",
  "main": "dist/already-styled-components.cjs.js",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "prepublish": "yarn lint && yarn build",
    "lint": "eslint --ext js,jsx src",
    "lint:fix": "eslint --ext js,jsx --fix src",
    "format": "prettier-eslint --write \"src/**/*.{js,jsx}\"",
    "test": "jest",
    "test:staged": "jest --findRelatedTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn format",
      "yarn test:staged",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "styled-components",
    "polished"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkosKon/already-styled-components.git"
  },
  "author": "AffectionateDoor",
  "license": "MIT",
  "dependencies": {
    "polished": "^2.3.3",
    "prop-types": "^15.6.2",
    "react-transition-group": "^2.5.3"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "styled-components": "^3.4.10 || ^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.22",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.0",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.2",
    "rollup-plugin-uglify": "^6.0.1",
    "styled-components": "^4.1.3"
  }
}
