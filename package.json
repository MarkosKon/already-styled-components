{
  "name": "already-styled-components",
  "version": "2.1.0",
  "description": "A collection of common UI components styled with styled-components.",
  "main": "dist/already-styled-components.js",
  "scripts": {
    "build": "rm -rf dist && webpack",
    "prepublish": "npm run lint && npm run build",
    "lint": "./node_modules/.bin/eslint --ext js,jsx src",
    "lintf": "./node_modules/.bin/eslint --ext js,jsx --fix src",
    "test": "jest"
  },
  "sideEffects": false,
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "react",
    "styled-components",
    "polished"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkosKon/already-styled-components.git"
  },
  "author": "AffectionateDoor",
  "license": "MIT",
  "dependencies": {
    "polished": "^2.3.1",
    "prop-types": "^15.6.2",
    "react-transition-group": "^2.5.1"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "styled-components": "^3.4.10 || ^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "pre-commit": "^1.2.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "styled-components": "^4.1.2",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  }
}
