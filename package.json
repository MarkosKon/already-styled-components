{
  "name": "already-styled-components",
  "version": "2.0.1",
  "description": "A collection of common UI components styled with styled-components.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && babel src -d dist",
    "prepublish": "npm run lint && npm run build",
    "lint": "./node_modules/.bin/eslint src/**/*",
    "lintf": "./node_modules/.bin/eslint --fix src/**/*",
    "test": "jest"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "react",
    "styled-components",
    "onecolor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkosKon/already-styled-components.git"
  },
  "author": "AffectionateDoor",
  "license": "MIT",
  "dependencies": {
    "onecolor": "^3.1.0",
    "prop-types": "^15.6.2",
    "react-transition-group": "^2.5.0"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "styled-components": "^3.4.10 || ^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^22.4.3",
    "pre-commit": "^1.2.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "styled-components": "^3.4.10 || ^4.0.3"
  }
}
